{
  "name": "@realm/bindgen",
  "version": "0.1.0",
  "private": true,
  "description": "Binding generator exposing Realm Core to the SDK",
  "bin": {
    "realm-bindgen": "./realm-bindgen.ts"
  },
  "type": "module",
  "scripts": {
    "start": "./realm-bindgen.ts",
    "generate:spec-schema": "typescript-json-schema src/models/relaxed-spec.ts RelaxedSpec --out generated/spec.schema.json --required",
    "generate:typescript": "./realm-bindgen.ts --spec src/tests/complex-spec.yml --template typescript --output generated/ts"
  },
  "dependencies": {
    "@types/node": "^12.20.55",
    "ajv": "^8.11.0",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "commander": "^9.3.0",
    "debug": "^4.3.4",
    "tsm": "^2.2.1",
    "typescript-json-schema": "^0.53.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7"
  }
}